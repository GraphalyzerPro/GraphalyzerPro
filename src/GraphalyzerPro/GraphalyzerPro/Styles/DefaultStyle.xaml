<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style TargetType="{x:Type Button}">
        <Setter Property="Width" Value="70"/>
    </Style>

    <Style x:Key="TabItemHeaderButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="Width" Value="15"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Background" Value="Transparent"/>
    </Style>

    <BitmapImage x:Key="NewTabIcon" UriSource="../Icons/icon_newtab_32x32.png" />
    <BitmapImage x:Key="CloseTabIcon" UriSource="../Icons/icon_close_32x32.png" />
    <BitmapImage x:Key="NewSessionIcon" UriSource="../Icons/icon_newsession_32x32.png" />
    
    
</ResourceDictionary>