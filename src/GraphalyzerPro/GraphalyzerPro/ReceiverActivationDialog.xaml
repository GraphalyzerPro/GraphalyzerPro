<Window x:Class="GraphalyzerPro.ReceiverActivationDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        DataContext="{Binding Path=ReceiverActivationDialogViewModel, Source={StaticResource ViewModelLocator}}"
        Title="Empfänger aktivieren" Height="330" Width="660" ShowInTaskbar="False">
    <Grid>
        <ListBox x:Name="lbReceiverTypes" Margin="10" ItemsSource="{Binding ReceiverTypes}" SelectedItem="{Binding SelectedReceiverType}"/>
    </Grid>
</Window>
